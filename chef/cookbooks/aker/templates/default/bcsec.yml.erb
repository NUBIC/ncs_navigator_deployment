<% if @pers_group && @pers_entry %>
cc_pers:
  bcdatabase:
    group: <%= @pers_group %>
    entry: <%= @pers_entry %>
<% end %>

<% if @netid_user && @netid_password %>
netid:
  user: <%= @netid_user %>
  password: <%= @netid_password %>
<% end %>

policy:
  lockout-duration-seconds: 600
  lockout-attempts: 5
cas:
  cas_base_url: <%= @cas_base_url %>
  proxy_retrieval_url: <%= @cas_proxy_retrieval_url %>
  proxy_callback_url: <%= @cas_proxy_callback_url %>
